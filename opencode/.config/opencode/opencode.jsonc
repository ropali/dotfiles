        },
        "qwen3:14b": {
          "name": "Qwen3 14B"
        },
        "mistral:7b": {
          "name": "Mistral 7B"
        },
        "qwen2.5-coder:14b": {
          "name": "Qwen2.5 Coder 14b"
        }
      }
    }
  }

}
